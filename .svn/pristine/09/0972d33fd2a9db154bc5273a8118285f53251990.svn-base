@charset "UTF-8";


body {
  padding: 10px 20px;
}
.menu-header {
  display: inline;
  font-size: 16px;
  font-weight: bold;
  padding: 8px;
}
.this {
  font-size: 16px;
  font-weight: bold;
  color: #004680;
}

.content-header {
  border-bottom: 5px solid #004680;
}
.card-header {
  margin-top: 20px;
  border-bottom: none;
}
.card-body {
  padding-top: 8px;
  height: 60vh;
}

.attcolor{
	color: #48a2de;
}

/* content-header 와 div row 공간  */

.dd {
	padding : 30px;
}

searchBox{
	border:none
	}
.searchBox tbody th{
	padding:20px 10px 20px 35px;
	font-size:14px;
	font-weight:bold;
	text-align:left;
	vertical-align:top;
	border:none;
	background:#e6e6e6 
}
.searchBox tbody td{
	padding:12px 10px 12px 25px;
	border:none;
	background-color:#efefef
}
        
.searchDate{
	overflow:hidden;margin-bottom:10px;*zoom:1
}
.searchDate:after{
	display:block;clear:both;content:''
}
.searchDate li{
	position:relative;float:left;margin:0 7px 0 0
}
.searchDate li .chkbox2{
	display:block;text-align:center
}
.searchDate li .chkbox2 input{
	position:absolute;z-index:-1
	}
.searchDate li .chkbox2 label{
	display:block;
	width:72px;
	height:26px;
	font-size:14px;
	font-weight:bold;
	color:#fff;
	text-align:center;
	line-height:25px;
	text-decoration:none;
	cursor:pointer;
}
        
.demi{
	display:inline-block;
	margin:0 1px;
	vertical-align:middle
}
.inpType{
	padding-left:6px;height:24px;line-height:24px;border:1px solid #dbdbdb
}
.btncalendar{
	display:inline-block;
	width:22px;
	height:22px;
	text-indent:-999em
}
