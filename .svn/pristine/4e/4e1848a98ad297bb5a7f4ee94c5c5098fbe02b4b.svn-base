	padding-top: 8px;
	height: 60vh;
}

.attcolor {
	color: #48a2de;
}

/* content-header 와 div row 공간  */
.dd {
	padding: 30px;
}
/*검색버튼  */
.btnnn {
	float: 50%;
	background: white;
	padding: 5 px 12 px;
	margin: 0 2 px;
	border: 2 px solid lightgrey;
	border-radius: 3 px;
	color: #5B5B5B;
	font-size: 13px;
	font-weight: bold;
}

#btnSearch{
	float: 50%;
	background: white;
	padding: 5 px 12 px;
	margin: 0 2 px;
	border: 2 px solid lightgrey;
	border-radius: 3 px;
	color: #5B5B5B;
	font-size: 13px;
	font-weight: bold;
	
}
/*조회하는 박스  */
.boxSearch {
	width: 100%;
	text-align: center;
	padding-left: 300px;
}
